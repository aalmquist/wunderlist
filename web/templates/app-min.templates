{"layout": "<style type=\"text/css\">\n @import url('/css/app-min.css');\n</style>\n\n<div id=\"layout\">\n\n <!-- Task Listings -->\n <div id=\"tasks\"></div>\n\n</div>", "list": "<li<%= (inbox === '1')? \" class=\\'inbox\\'\" : \"\" %>>\n <label><%=label%></label>\n <span class=\"taskCount\"><%=task_count%></span>\n <% if(inbox !== '1') { %>\n <a class=\"share\"></a>\n <a class=\"delete\"></a>\n <% } %>\n <a class=\"edit\"></a>\n <a class=\"save\"></a>\n</li>", "sharing": "<!-- Print / Share controls -->\n<div id=\"sharing\">\n <a rel=\"print tasks\" class=\"icon print\">Print Tasks</a>\n <a rel=\"send by email\" class=\"icon email\">Send by Email</a>\n <a rel=\"share with cloud app\" class=\"icon cloud\">Share with Cloud App</a>\n <div class=\"tooltip cloudtip\">\n <span class=\"triangle\"></span>\n <span class=\"copy\">Copy Link</span>\n <span class=\"link\"></span>\n </div>\n</div>\n<!-- Extra DOM for printing/sharing -->\n<div id=\"extras\">\n <iframe id=\"printFrame\"></iframe>\n</div>", "sidebar": "<!-- SideBar -->\n<div id=\"sidebar\">\n <div class=\"searchSide\">\n <input type=\"text\" id=\"search\" placeholder=\"Search for tasks\" autocomplete='off' />\n <span class=\"icon lens\"></span>\n <span class=\"icon clearSearch\"></span>\n </div>\n <ol id=\"lists\"></ol>\n <div id=\"notification\"><div></div></div>\n</div>", "bottombar": "<!-- Bottom Panel -->\n<div id=\"bottomBar\">\n <div class=\"leftBottom\">\n <span class=\"icon sync\">\n <span class=\"icon syncing\"></span>\n </span>\n <ul class=\"menu\"></ul>\n <span id=\"filters\"></span>\n </div>\n <div class=\"rightBottom\">\n <a class=\"addList\" title=\"Add a new List\">Add List</a>\n <a class=\"icon showHelp\">?</a>\n <a class=\"icon backgroundSwitcher\">\n <span class=\"icon activeBackground\"></span>\n <span class=\"backgroundList\"></span>\n </a>\n <a class=\"icon toggleSidebar\" title=\"Toggle Sidebar\"></a>\n </div>\n</div>"}